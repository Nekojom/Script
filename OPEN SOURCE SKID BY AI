end)

--== [ INTAST UGC V2 - HIGH CHANCE CLAIM ] ==--

local function intastUGCV2()

	local hrp = Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

	local total = 0

	for _, obj in ipairs(game:GetDescendants()) do

		pcall(function()

			if obj:IsA("ProximityPrompt") and obj.Enabled then

				fireproximityprompt(obj)

				total += 1

			elseif obj:IsA("TextButton") and obj.Visible and obj.Text and obj.Text:lower():find("claim") then

				obj:Activate()

				total += 1

			elseif obj:IsA("BasePart") and obj:FindFirstChildOfClass("TouchTransmitter") then

				firetouchinterest(hrp, obj, 0)

				wait(0.05)

				firetouchinterest(hrp, obj, 1)

				total += 1

			elseif obj:IsA("ClickDetector") then

				fireclickdetector(obj)

				total += 1

			end

		end)

	end

	notify("üíé INTAST UGC V2", "Total processed: " .. total)

end

local btnIntast2 = Instance.new("TextButton", scroll)

btnIntast2.Size = UDim2.new(0, 260, 0, 40)

btnIntast2.Position = UDim2.new(0, 20, 0, 550)

btnIntast2.Text = "üíé INTAST UGC V2 (MAX)"

btnIntast2.BackgroundColor3 = Color3.fromRGB(200, 200, 0)

btnIntast2.TextColor3 = Color3.new(0, 0, 0)

btnIntast2.Font = Enum.Font.GothamBold

btnIntast2.TextScaled = true

btnIntast2.MouseButton1Click:Connect(intastUGCV2)

--== [ TELEPORT TO UGC PARTS ] ==--

local function teleportToUGC()

	local hrp = Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

	for _, obj in ipairs(workspace:GetDescendants()) do

		if obj:IsA("BasePart") and (

			obj:FindFirstChildOfClass("TouchTransmitter")

			or obj:FindFirstChildOfClass("ClickDetector")

		) then

			hrp.CFrame = obj.CFrame + Vector3.new(0, 3, 0)

			wait(0.3)

		end

	end

	notify("üöÄ Teleport Done", "Finished teleporting to UGC parts.")

end

local btnTP = Instance.new("TextButton", scroll)

btnTP.Size = UDim2.new(0, 260, 0, 40)

btnTP.Position = UDim2.new(0, 20, 0, 600)

btnTP.Text = "üöÄ TELEPORT TO UGC"

btnTP.BackgroundColor3 = Color3.fromRGB(100, 100, 255)

btnTP.TextColor3 = Color3.new(1, 1, 1)

btnTP.Font = Enum.Font.GothamBold

btnTP.TextScaled = true

btnTP.MouseButton1Click:Connect(teleportToUGC)

--== [ ESP UGC Part Viewer ] ==--

local function createESP(part, name)

	local BillboardGui = Instance.new("BillboardGui")

	BillboardGui.Parent = part

	BillboardGui.Adornee = part

	BillboardGui.Size = UDim2.new(0, 100, 0, 20)

	BillboardGui.AlwaysOnTop = true

	local TextLabel = Instance.new("TextLabel")

	TextLabel.Parent = BillboardGui

	TextLabel.Size = UDim2.new(1, 0, 1, 0)

	TextLabel.Text = name

	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 0)

	TextLabel.BackgroundTransparency = 1

	TextLabel.TextScaled = true

end

local function espUGC()

	local count = 0

	for _, obj in ipairs(workspace:GetDescendants()) do

		if obj:IsA("BasePart") and (

			obj:FindFirstChildOfClass("TouchTransmitter")

			or obj:FindFirstChildOfClass("ClickDetector")

		) and not obj:FindFirstChildWhichIsA("BillboardGui") then

			createESP(obj, "UGC Part")

			count += 1

		end

	end

	notify("üëÅÔ∏è ESP Enabled", count .. " UGC parts marked.")

end

local btnESP = Instance.new("TextButton", scroll)

btnESP.Size = UDim2.new(0, 260, 0, 40)

btnESP.Position = UDim2.new(0, 20, 0, 650)

btnESP.Text = "üëÅÔ∏è ESP UGC PARTS"

btnESP.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

btnESP.TextColor3 = Color3.fromRGB(0, 255, 255)

btnESP.Font = Enum.Font.GothamBold

btnESP.TextScaled = true

btnESP.MouseButton1Click:Connect(espUGC)

local function brutalIntastScanner()

	local canClaim = false

	local claimables = {}

	for _, obj in ipairs(game:GetDescendants()) do

		if obj:IsA("TextButton") and obj.Visible and obj.Text and obj.Text:lower():find("claim") then

			table.insert(claimables, obj)

			canClaim = true

		elseif obj:IsA("ProximityPrompt") and obj.Enabled then

			table.insert(claimables, obj)

			canClaim = true

		elseif obj:IsA("BasePart") then

			if obj:FindFirstChildOfClass("TouchTransmitter") or obj:FindFirstChildOfClass("ClickDetector") then

				table.insert(claimables, obj)

				canClaim = true

			end

		end

	end

	if canClaim then

		notify("‚úÖ INTAST UGC Detected!", "Game ini bisa di-claim secara brutal!")

	else

		notify("‚ùå No UGC Claim Found", "Tidak ada tombol atau sistem UGC yang bisa diakses.")

	end

	-- Brutal claim process

	for _, obj in ipairs(claimables) do

		if obj:IsA("TextButton") then

			pcall(function() obj:Activate() end)

		elseif obj:IsA("ProximityPrompt") then

			pcall(function() fireproximityprompt(obj) end)

		elseif obj:IsA("BasePart") then

			local pos = obj.Position or obj.Parent.Position

			hrp.CFrame = CFrame.new(pos + Vector3.new(0, 3, 0))

			wait(0.1)

			pcall(function()

				if obj:FindFirstChildOfClass("TouchTransmitter") then

					firetouchinterest(hrp, obj, 0)

					wait()

					firetouchinterest(hrp, obj, 1)

				end

				if obj:FindFirstChildOfClass("ClickDetector") then

					fireclickdetector(obj:FindFirstChildOfClass("ClickDetector"))

				end

			end)

		end

	end

end

local btnBrutalScan = Instance.new("TextButton", scroll)

btnBrutalScan.Size = UDim2.new(0, 260, 0, 40)

btnBrutalScan.Position = UDim2.new(0, 20, 0, 550)

btnBrutalScan.Text = "üß† INTAST SCAN & AUTO BYPASS"

btnBrutalScan.BackgroundColor3 = Color3.fromRGB(0, 255, 200)

btnBrutalScan.TextColor3 = Color3.new(0,0,0)

btnBrutalScan.Font = Enum.Font.GothamBold

btnBrutalScan.TextScaled = true

btnBrutalScan.MouseButton1Click:Connect(brutalIntastScanner)

