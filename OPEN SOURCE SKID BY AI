obbyInput.TextScaled = true

-- Bagian Function & Logic

local logClaims = {}

local function notify(title, text)

StarterGui:SetCore("SendNotification", {

Title = title,

Text = text,

Duration = 5

})

end

-- Logger Hook

local function logHook(object)

if not table.find(logClaims, object) then

table.insert(logClaims, object)

print("üìå Hooked:", object:GetFullName())

end

end

-- INTAST CLAIM

local function claimUGC()

local count = 0

for _, obj in ipairs(game:GetDescendants()) do

if obj:IsA("TextButton") and obj.Visible and obj.Text and obj.Text:lower():find("claim") then

pcall(function()

logHook(obj)

obj:Activate()

end)

count += 1

elseif obj:IsA("ProximityPrompt") and obj.Enabled then

pcall(function()

logHook(obj)

fireproximityprompt(obj)

end)

count += 1

end

end

notify(count > 0 and "‚úÖ UGC Claimed!" or "‚ùå Nothing Found", "Claimed: " .. count)

end

-- DETECT UGC

local function scanUGC()

local found = 0

for _, obj in ipairs(game:GetDescendants()) do

if obj:IsA("TextButton") and obj.Text and (obj.Text:lower():find("claim") or obj.Text:lower():find("get")) then

found += 1

end

end

notify("üîç Scan Done", found > 0 and ("Found " .. found .. " UGC buttons!") or "No UGC detected.")

end

-- AUTO FIX LOOP

local autoFixing = false

local function autoFixLoop()

autoFixing = not autoFixing

notify(autoFixing and "üîÅ Auto Fix ON" or "üîÅ Auto Fix OFF", "Loop claim every 2 seconds.")

while autoFixing do

claimUGC()

wait(2)

end

end

-- AUTO OBBY PATH

local function autoObby(stageNumber)

local foundStage = false

for _, obj in ipairs(workspace:GetDescendants()) do

if obj:IsA("BasePart") and obj.Name:lower():find("stage") and tostring(obj.Name):find(tostring(stageNumber)) then

local path = PathfindingService:CreatePath()

path:ComputeAsync(hrp.Position, obj.Position)

if path.Status == Enum.PathStatus.Complete then

for _, waypoint in ipairs(path:GetWaypoints()) do

hrp.CFrame = CFrame.new(waypoint.Position)

wait(0.1)

end

notify("‚úÖ Stage Complete", "Reached stage: " .. stageNumber)

foundStage = true

break

end

end

end

if not foundStage then

notify("‚ùå Stage Not Found", "Stage " .. stageNumber .. " tidak ditemukan.")

end

end

-- Sambungkan tombol ke fungsi masing-masing

btnClaim.MouseButton1Click:Connect(claimUGC)

btnScan.MouseButton1Click:Connect(scanUGC)

btnFix.MouseButton1Click:Connect(function()

coroutine.wrap(autoFixLoop)()

end)

-- Fokus Lost untuk Auto Obby

obbyInput.FocusLost:Connect(function(enter)

if enter then

local stage = tonumber(obbyInput.Text)

if stage then

autoObby(stage)

else

notify("‚ö†Ô∏è Invalid Input", "Masukkan angka stage 1‚Äì1000.")

end

end

end)

local function autoTouchUGC()

local touched = 0

for _, part in ipairs(workspace:GetDescendants()) do

if part:IsA("BasePart") and part:FindFirstChildOfClass("TouchTransmitter") then

pcall(function()

firetouchinterest(hrp, part, 0)

wait(0.1)

firetouchinterest(hrp, part, 1)

end)

touched += 1

end

end

notify("üß≤ Auto Touch Done", "Touched: " .. touched .. " parts.")

end

local function detectUGCStrength()

local hooked = 0

for _, obj in ipairs(game:GetDescendants()) do

if obj:IsA("TextButton") and obj.Visible and obj.Text and obj.Text:lower():find("claim") then

local suc, res = pcall(function()

return getconnections(obj.MouseButton1Click)

end)

if suc and res and #res > 0 then

hooked += 1

end

end

end

notify("üß† UGC Hook Analysis", hooked > 0 and hooked .. " buttons have hooks." or "No hook found.")

end

local function spoofHookSniper()

for _, obj in ipairs(game:GetDescendants()) do

if obj:IsA("TextButton") and obj.Visible and obj.Text and obj.Text:lower():find("claim") then

local conns = getconnections(obj.MouseButton1Click)

for i, conn in ipairs(conns) do

print("üé£ Hook Sniped:", conn.Function)

end

end

end

notify("üé£ Spoofer Scan", "Logged all hooks to console.")

end

local collected = {}

local function logUGC(object)

if not table.find(collected, object) then

table.insert(collected, object)

print("‚úÖ Collected UGC:", object:GetFullName())

end

end

local function claimAllUGC()

claimUGC()

wait(0.5)

autoTouchUGC()

end

